def solution(wallpaper):
    pointer_list = []
    y_pointer = []
    x_pointer = []
    n = 0
    s = 0
    w = 0
    e = 0
    for i, row in enumerate(wallpaper):
        for j, point in enumerate(row):
            if point == '#':
                pointer_list.append([i,j])
    print(pointer_list)
    for pointer in pointer_list:
        y_pointer.append(pointer[0])
        x_pointer.append(pointer[1])
    print(y_pointer)
    print(x_pointer)
    n = min(y_pointer)
    s = max(y_pointer) + 1
    w = min(x_pointer)
    e = max(x_pointer) + 1
    
    return [n,w,s,e]
                
